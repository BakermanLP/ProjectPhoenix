<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<config>
    <lines at="topleft">
        <line>
            <str>Day {day}, {mctime} (</str>
            <if>
                <var>daytime</var>
                <str>$eDay</str>
                <str>$8Night</str>
            </if>
            <str> time$f)</str>
        </line>
        <line>
            <str>Biome: $e{biome}$f</str>
            <if>
                <var>slimes</var>
                <str> ($aSlime$f)</str>
                <str> </str>
            </if>
        </line>
        <line>
            <str>Light: </str>
            <max>
                <var>light</var>
                <num>7.5</num>
                <str>$e</str>
                <str>$c</str>
            </max>
            <var>light</var>
            <str>$r (feet: </str>
            <max>
                <var>lightfeet</var>
                <num>7.5</num>
                <str>$e</str>
                <str>$c</str>
            </max>
            <var>lightfeet</var>
            <str>$r)</str>
        </line>
        <line>
            <str>Entities: $e{entitiesrendered}$f / $e{entitiestotal}$f</str>
        </line>
    </lines>
</config>
